<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cop.mobi.mycar.service.dao.DiagnoseDao">
	<resultMap id="diagnoseItemResult" type="com.cop.mobi.common.KeyValuePair">
		<result property="key" column="obdCode" />
		<result property="value" column="description" />
	</resultMap>

	<select id="getAllDiagnoseItems" resultMap="diagnoseItemResult">
		SELECT
		obdCode, description
		FROM
		LC_OBDCode
	</select>
	
</mapper>